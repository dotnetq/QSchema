<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/machonky/qtools/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/machonky/qtools</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Company>MacRobert.com</Company>
    <Authors>dev@macrobert.com</Authors>
    <Description>Create kdb schema directly from C# data models.</Description>
    <Version>1.0.6</Version>
    <Copyright>macrobert.com</Copyright>
    <AssemblyVersion>1.0.6.0</AssemblyVersion>
    <FileVersion>1.0.6.0</FileVersion>
    <PackageReleaseNotes>Formal foreign key support - as long as the dependencies can be mapped as a DAG</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="QTools.Schema" Version="1.0.5" />
    <PackageReference Include="TopologicalSorting.netstandard" Version="1.0.3" />
  </ItemGroup>

</Project>
